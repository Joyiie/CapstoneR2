@page
@model CapstoneR2.Pages.Manage.Apps.EditModel
@{
}
<div class="row">
    <div class="col-4">
        <div class="card">
            <div class="card-body">
                <form method="Post">
                    <div class="form-group">
                        <input type="hidden" asp-for="View.ID" />
                        <label asp-for="View.Symptom">Symptoms</label>
                        <input type="text" class="form-control" placeholder="Enter role name" asp-for="View.Symptom">
                    </div>
                  
                    <div class="form-group">
                        <label asp-for="View.StartTime">StartTime</label>
                        <input type="datetime-local" class="form-control" placeholder="DateTime" asp-for="View.StartTime">
                    </div>

                    <div class="form-group">
                        <label asp-for="View.EndTime">EndTime</label>
                        <input type="datetime-local" class="form-control" placeholder="DateTime" asp-for="View.EndTime">
                    </div>
                 
                  
                    <div class="form-group alert-danger">
                        @Html.ValidationSummary()
                    </div>
                    <div class="form-group">
                        <button type="submit" class="btn btn-primary">Edit</button>
                        <a href="~/manage/patients" class="btn btn-secondary">Cancel</a>
                    </div>


                </form>
            </div>
        </div>
    </div>
</div>