@page
@model CapstoneR2.Pages.Manage.Consultations.CUpdate
@{
}
<div class="row">
    <div class="col-4">
        <div class="card">
            <div class="card-body">
                <form method="Post">
                    <div class="form-group">
                        <input type="hidden" asp-for="View.ID" />
                        <label asp-for="View.Symptoms">Symptoms</label>
                        <input type="text" class="form-control" placeholder="Enter role name" asp-for="View.Symptoms">
                    </div>
                   
                    
                    <div class="form-group">
                        <label asp-for="View.DateCreated">DateCreated</label>
                        <input type="datetime-local" class="form-control" placeholder="DateTime" asp-for="View.DateCreated">
                    </div>
                    <div class="form-group">
                        <label asp-for="View.DateUpdated">DateUpdated</label>
                        <input type="datetime-local" class="form-control" placeholder="DateTime" asp-for="View.DateUpdated">
                    </div>
                    

                    
                    
                   
                    <div class="form-group alert-danger">
                        @Html.ValidationSummary()
                    </div>
                    <div class="form-group">
                        <button type="submit" class="btn btn-primary">Update</button>
                        <a href="~/manage/consultations" class="btn btn-secondary">Cancel</a>
                    </div>


                </form>
            </div>
        </div>
    </div>
</div>